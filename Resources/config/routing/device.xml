<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns="http://symfony.com/schema/routing" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">
  <route id="rednose_framework_device_ios_subscribe" pattern="/device/ios/subscribe">
    <default key="_controller">RednoseFrameworkBundle:Device:iosSubscribe</default>
    <requirement key="_method">POST</requirement>
  </route>
  <route id="rednose_framework_device_ios_unsubscribe" pattern="/device/ios/unsubscribe">
    <default key="_controller">RednoseFrameworkBundle:Device:iosUnsubscribe</default>
    <requirement key="_method">POST</requirement>
  </route>
</routes>
