<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns="http://symfony.com/schema/routing" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">
  <route id="_libbit_framework_security_login" pattern="/login">
    <default key="_controller">LibbitFrameworkBundle:Security:login</default>
  </route>
  <route id="_libbit_framework_security_check" pattern="/login_check">
    <default key="_controller">LibbitFrameworkBundle:Security:check</default>
  </route>
  <route id="_libbit_framework_security_logout" pattern="/logout">
    <default key="_controller">LibbitFrameworkBundle:Security:logout</default>
  </route>
</routes>
