{% extends 'RednoseFrameworkBundle::layout.html.twig' %}

{% block content %}
    <script>
        YUI_config.lang = 'nl-NL';

        YUI().use('rednose-form-designer', function (Y) {
            new Y.Rednose.FormDesigner.FormDesigner({
                serverRouting: true,
                transitions  : true,
                root         : '{{ admin.generateUrl('list')|replace({'/list': ''}) }}'
            }).render();
        });
    </script>
{% endblock %}
