{
    "name": "libbit/framework-bundle",
    "type": "symfony-bundle",
    "description": "Symfony LibbitFrameworkBundle",
    "keywords": ["framework"],
    "homepage": "http://www.libbit.eu",
    "license": "EUPL-1.1",
    "authors": [
        {
            "name": "Marc Bontje",
            "email": "marc@rednose.nl",
            "homepage": "www.rednose.nl"
        },
        {
            "name": "Sven Hagemann",
            "email": "sven@rednose.nl",
            "homepage": "www.rednose.nl"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packagist.rednose.nl/"
        },
        {
            "type": "package",
            "package": {
                "name": "sonata-project/block-bundle",
                "version": "2.0.x-dev",
                "source": {
                    "url": "https://github.com/sonata-project/SonataBlockBundle",
                    "type": "git",
                    "reference": "2.0"
                },
                "require": {
                    "php": ">=5.3.2",
                    "symfony/framework-bundle": "2.*",
                    "sonata-project/cache-bundle": "2.0.*"
                },
                "autoload": {
                    "psr-0": { "Sonata\\BlockBundle": "" }
                },
                "target-dir": "Sonata/BlockBundle"
            }
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "2.0.11",
        "symfony/framework-bundle": "2.0.*",
        "symfony/security-bundle": "2.0.*",
        "doctrine/doctrine-fixtures-bundle": "2.0.*",
        "friendsofsymfony/user-bundle": "1.2.*",
        "sonata-project/intl-bundle": "dev-master",
        "sonata-project/doctrine-orm-admin-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Libbit\\FrameworkBundle": "" }
    },
    "target-dir": "Libbit/FrameworkBundle"
}
